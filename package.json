{
  "name": "react-manual-environment",
  "version": "1.0.0",
  "description": "Manual minimal environment of react project",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "concurrently \"npm run dev-server\" \"npm run dev-bundle\"",
